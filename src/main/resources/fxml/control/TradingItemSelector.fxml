<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" type="javafx.scene.layout.VBox" spacing="5"
         xmlns="http://javafx.com/javafx/17.0.2-ea">
    <TextField fx:id="tradingItemField" promptText="%tradingItemSelector.label.catalogNumber"/>
    <ComboBox fx:id="categoryComboBox" disable="true" maxWidth="Infinity"
              promptText="%tradingItemSelector.label.category"/>
</fx:root>
