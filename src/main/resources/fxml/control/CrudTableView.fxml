<?xml version="1.0" encoding="UTF-8"?>

<?import com.philabid.ui.control.CrudToolbar?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" type="javafx.scene.layout.VBox"
         xmlns="http://javafx.com/javafx/17.0.2-ea">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <Label fx:id="titleLabel" style="-fx-font-size: 16px; -fx-font-weight: bold;"/>
    <CrudToolbar fx:id="crudToolbar"/>
    <TableView fx:id="tableView" VBox.vgrow="ALWAYS">
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
    </TableView>
</fx:root>
