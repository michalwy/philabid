<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/17.0.2-ea"
          fx:controller="com.philabid.ui.ArchiveAuctionDialogController">
    <padding>
        <Insets bottom="10" left="10" right="10" top="10"/>
    </padding>
    <Label text="%archive.dialog.finalPrice" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
    <TextField fx:id="finalPriceField" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
    <ButtonBar GridPane.columnSpan="2" GridPane.rowIndex="1">
        <buttons>
            <Button defaultButton="true" onAction="#handleSave" text="%common.save"/>
            <Button cancelButton="true" onAction="#handleCancel" text="%common.cancel"/>
        </buttons>
    </ButtonBar>
</GridPane>