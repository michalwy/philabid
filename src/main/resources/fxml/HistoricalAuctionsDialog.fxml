<?xml version="1.0" encoding="UTF-8"?>

<?import com.philabid.ui.control.ValuationDetails?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" spacing="10" xmlns="http://javafx.com/javafx/17.0.2-ea"
      fx:controller="com.philabid.ui.HistoricalAuctionsDialogController">
    <padding>
        <Insets bottom="10" left="10" right="10" top="10"/>
    </padding>

    <ValuationDetails fx:id="valuationDetails" VBox.vgrow="ALWAYS"/>

    <ButtonBar>
        <buttons>
            <Button cancelButton="true" onAction="#handleCancel" text="%common.cancel"/>
        </buttons>
    </ButtonBar>
</VBox>