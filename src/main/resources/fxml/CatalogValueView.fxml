<?xml version="1.0" encoding="UTF-8"?>

<?import com.philabid.ui.control.CrudTableView?>
<?import javafx.scene.control.*?>
<CrudTableView xmlns:fx="http://javafx.com/fxml/1" fx:id="crudTableView" spacing="10.0"
               xmlns="http://javafx.com/javafx/17.0.2-ea"
               fx:controller="com.philabid.ui.CatalogValueController" title="%catalogValues.title">
    <columns>
        <TableColumn fx:id="categoryColumn" prefWidth="200.0" text="%catalogValues.column.category"/>
        <TableColumn fx:id="catalogNumberColumn" prefWidth="200.0" text="%catalogValues.column.catalogNumber"/>
        <TableColumn fx:id="conditionColumn" prefWidth="150.0" text="%catalogValues.column.condition"/>
        <TableColumn fx:id="catalogColumn" prefWidth="200.0" text="%catalogValues.column.catalog"/>
        <TableColumn fx:id="valueColumn" prefWidth="100.0" text="%catalogValues.column.value"/>
    </columns>
</CrudTableView>
